generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        String   @id @default(cuid()) 
  name      String
  email     String   @unique
  role      String   // will be an enum (LESSOR/RENTOR) lol
  createdAt DateTime @default(now())
}